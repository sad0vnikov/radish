  "repository": {
    "type": "git",
    "url": "https://github.com/sad0vnikov/radish"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "rimraf dist && webpack && mv dist/*.eot dist/static/css/ && mv dist/*.svg dist/static/css/ && mv dist/*.ttf dist/static/css/",
